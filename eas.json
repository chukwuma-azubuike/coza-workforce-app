{
    "cli": {
        "version": ">= 15.0.6",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "APP_VARIANT": "development",
                "SENTRY_DNS": "SENTRY_DNS",
                "EXPO_APPLE_TEAM_ID": "499THBRLGD",
                "NODE_VERSION": "20"
            },
            "channel": "development"
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "APP_VARIANT": "preview",
                "SENTRY_DNS": "SENTRY_DNS",
                "NODE_VERSION": "20"
            },
            "channel": "preview"
        },
        "production": {
            "env": {
                "APP_VARIANT": "production",
                "SENTRY_DNS": "SENTRY_DNS",
                "NODE_VERSION": "20"
            },
            "autoIncrement": true,
            "ios": {
                // "prebuildCommand": "npx pod-install"
            },
            "channel": "production"
        }
    },
    "submit": {
        "ios": {
            // "ascAppId": "1667229564"
        }
    }
}
